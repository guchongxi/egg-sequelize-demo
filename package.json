{
  "name": "egg-sequelize-demo",
  "version": "1.0.0",
  "description": "在 Egg 中使用 sequelize 示例项目",
  "dependencies": {
    "egg": "^2.10.0",
    "egg-sequelize": "^5.0.1",
    "mysql2": "^1.6.1"
  },
  "devDependencies": {
    "egg-bin": "^4.8.1",
    "sequelize-cli": "^5.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "dev": "egg-bin dev --port=7005",
    "db-init": "sequelize db:migrate && sequelize db:seed:all",
    "db-reset": "sequelize db:migrate:undo:all"
  },
